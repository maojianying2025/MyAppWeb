{
  "name": "Customer",
  "type": "object",
  "properties": {
    "region": {
      "type": "string",
      "description": "Region"
    },
    "channel": {
      "type": "string",
      "description": "Channel"
    },
    "sub_channel": {
      "type": "string",
      "description": "Sub-channel"
    },
    "customer_name": {
      "type": "string",
      "description": "Customer name (uppercase required)",
      "pattern": "^[A-Z\\s]+$"
    },
    "code": {
      "type": "string",
      "description": "Customer code"
    },
    "follow_up_sss": {
      "type": "string",
      "description": "Follow-up SSS"
    },
    "population": {
      "type": "number",
      "description": "Population"
    },
    "longitude": {
      "type": "number",
      "description": "Longitude"
    },
    "latitude": {
      "type": "number",
      "description": "Latitude"
    },
    "signal": {
      "type": "string",
      "description": "Signal strength"
    },
    "address": {
      "type": "string",
      "description": "Address"
    },
    "program": {
      "type": "string",
      "description": "Related Program"
    },
    "contact_person": {
      "type": "string",
      "description": "Contact person"
    },
    "contact_phone": {
      "type": "string",
      "description": "Contact phone"
    },
    "contact_email": {
      "type": "string",
      "format": "email",
      "description": "Contact email"
    },
    "nomination": {
      "type": "string",
      "description": "Nomination"
    }
  },
  "required": [
    "customer_name",
    "code"
  ]
}